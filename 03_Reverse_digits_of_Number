import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{
                Scanner sc = new Scanner(System.in);
                int n = sc.nextInt();
                int rev = 0;
                int digit;
                while(n>0){
                        digit = n%10;
                        rev = rev * 10 + digit;
                        n = n/10;
                }
                System.out.println(rev);
	}
}
